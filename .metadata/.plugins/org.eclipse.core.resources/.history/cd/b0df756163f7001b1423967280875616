package Arithmetic;

public class Substration_H extends Arith{
	//a = rand()%8+1;  //8  7
			//b = 10 - (rand()%(9-a)+1);  // 9   8,9
			a = rand()%8;  // (0,1,2,3,4,5,6,7) 7
			tmp = 8-a;
			b = 10 - (rand()%tmp+1);  // 9  9,8  
			a += 10;
			result = a-b;
	@Override
	void present(int random) {
		a = random % 8;
		b = 10 - (random%(8-a)+1);
		a += 10;
		result = a-b;	
	}

}
