package Arithmetic;

public abstract class Arith {
	protected int a;
	protected int b;
	protected int result;
	protected int q;
	protected int r;
	
	public int get_a()
	{
		return this.a;
	}
	public int get_b()
	{
		return this.b;
	}
	
	public int get_result()
	{
		return this.result;
	}
	
	public int get_r()
	{
		return this.result;
	}
	
	public int get_r()
	{
		return this.result;
	}
	
	abstract public void present(int random);
}
